      "json"
    ],
    "notify": true,
    "notifyMode": "always",
    "verbose": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "./node_modules/*",
      "./build"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
