  "homepage": "https://redfoxfinn.github.io/",
  "devDependencies": {
    "apollo-client": "^2.6.4",
    "apollo-utilities": "latest",
    "axios": "^0.18.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cors": "^2.8.5",
    "easygraphql-tester": "^5.1.6",
    "eslint": "^6.6.0",
    "graphql-tag": "latest",
