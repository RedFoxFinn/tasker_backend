  "engines": {
    "node": "11.x",
    "npm": "6.x"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.7",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "bson": "^4.0.1",
    "bson-ext": "^2.0.3",
    "dotenv": "^6.2.0",
